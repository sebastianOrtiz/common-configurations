{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "lazy-bundles",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/chunk-*.js"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-fresh",
      "urls": [
        "/api/method/frappe.auth.get_logged_user",
        "/api/method/frappe.auth.logout"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1m",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "/api/resource/**",
        "/api/method/common_configurations.api.**",
        "/api/method/meet_scheduling.api.**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "strategy": "performance",
        "timeout": "10s"
      }
    }
  ]
}
